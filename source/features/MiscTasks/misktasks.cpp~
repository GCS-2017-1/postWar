#include <iostream>

using namespace std;
int compara_selecao(int x_min,int x_max,int x,int y_min,int y_max,int y){
	cout<<"x_min: "<<x_min<<" x_max: "<<x_max<<" x: "<<x<<endl;
	cout<<"y_min: "<<y_min<<" y_max: "<<y_max<<" y: "<<y<<endl;
    if(x >= x_min && x <= x_max && y >= y_min && y<= y_max){
cout<<"fpo"<<endl;
        return 1;
    }
        else{
		
            return 0;
        }

}
